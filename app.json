{
  "expo": {
    "name": "TicTacTusi",
    "slug": "SnakyTusi",
    "version": "1.1.1",
    "orientation": "portrait",
    "icon": "./assets/images/appiconttt.png",
    "scheme": "snakytusi",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 4,  
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/appiconttt.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tstads.SnakyTusi"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/appiconttt.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c580ea92-e1d7-413a-ba4c-c8cab940ab76"
      }
    }
  }
}
